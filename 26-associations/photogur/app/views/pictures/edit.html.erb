<h2> Edit picture <%= @picture.id %> </h2>
<%= form_for @picture do |f| %>
  <div class="field">
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title %></p>
  </div>

  <div class="field">
    <p><%= f.label :description %></p>
    <p><%= f.text_field :description %></p>
  </div>

  <div class="field">
    <p><%= f.label :url %></p>
    <p><%= f.text_field :url %></p>
  </div>

  <div class="field">
    <p><%= f.submit  %></p>
  </div>
<% end %>
